{
  "name": "webpack-rails-example",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "imports-loader": "^0.6.5",
    "stats-webpack-plugin": "^0.2.1",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "mocha-webpack": "^0.3.1"
  },
  "scripts": {
    "test": "npm test:unit && npm run test:karma",
    "test:unit": "NODE_ENV=test NODE_PATH='./app/assets/javascripts' node_modules/.bin/mocha-webpack",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:browser": "NODE_ENV=test NODE_PATH='./app/assets/javascripts' webpack-dev-server --config config/webpack.config.test.browser.js --hot --inline",
    "test:karma": "NODE_ENV=test NODE_PATH='./app/assets/javascripts' karma start",
    "test:karma:watch": "npm run test:karma -- --auto-watch --no-single-run"
  }
}
